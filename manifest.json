{
    "manifest_version": 3,
    "name": "Cambia ceviche y pisco a ceviche peruano y pisco peruano",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["replace-words.js"],
        "run_at": "document_end"
      }
